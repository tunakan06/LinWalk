# 1. masterブランチに戻る
git checkout master

# 2. .gitignore に追加
echo "*.csproj" >> .gitignore
echo "*.sln" >> .gitignore
echo "Library/" >> .gitignore
echo "Temp/" >> .gitignore
echo "Logs/" >> .gitignore

# 3. 追跡を解除
git rm -r --cached Library/
git rm --cached *.csproj
git rm --cached *.sln

# 4. コミット
git add .gitignore
git commit -m "Unity自動生成ファイルをgit管理から除外"

# 5. プッシュ
git push
